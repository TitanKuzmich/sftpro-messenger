{
  "name": "server-side_packages",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Krivonos Aleksandr",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build/*",
    "build": "gulp build",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint:all": "yarn lint && yarn tscCheck",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint": "./node_modules/.bin/eslint src/**/*.ts",
    "start": "cross-env NODE_ENV=development nodemon",
    "server": "ts-node --files src/server.ts",
    "cli": "ts-node --files src/utils/cli/index.ts",
    "socket": "ts-node --files src/socket/server.ts",
    "type": "tsc --project tsconfig.json --noEmit",
    "seed:test": "cross-env NODE_ENV=test ts-node --transpile-only --files src/database/seeder.ts",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --testTimeout=10000",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage"
  },
  "devDependencies": {
    "@types/async": "^3.2.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/faker": "^5.5.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/method-override": "^0.0.32",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/request-ip": "^0.0.37",
    "@types/yamljs": "^0.2.31",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.20.0",
    "colors": "^1.4.0",
    "commander": "^8.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validation": "^3.0.8",
    "express-winston": "^4.2.0",
    "faker": "^5.5.0",
    "form-data": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.2",
    "json2csv": "^5.0.6",
    "json2yaml": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.0",
    "morgan": "^1.10.0",
    "request-ip": "^2.1.3",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "tslint": "^6.1.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
